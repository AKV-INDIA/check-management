<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="account_check_payment_transaction_user_rule" model="ir.rule">
            <field name="name">Access own check payment transaction only</field>
            <field name="model_id" ref="account_check_payment.model_check_payment_transaction"/>
            <field name="domain_force">[
                '|',
                    ('partner_id','=',False),
                    ('partner_id','=',user.partner_id.id)
                ]</field>
            <field name="groups" eval="[(4, ref('base.group_user')), (4, ref('base.group_portal')), (4, ref('base.group_public'))]"/>
        </record>
    </data>
</odoo>
